{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "My lovely Nuxt.js Template",
  "author": "krist7599555",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "TS_NODE_FILES=true ts-node --project tsconfig.json server/index.ts",
    "start:backend": "SKIP_NUXT=1 && yarn start",
    "dev": "TS_NODE_FILES=true nodemon --watch . --exec ts-node -e ts --ignore '*.test.ts' --project tsconfig.json index.ts",
    "dev:backend": "kill-port 3000 && SKIP_NUXT=1 yarn dev",
    "dev:backend:nowarn": "NODE_NO_WARNINGS=1 yarn dev:backend",
    "compile": "tsc --w",
    "production": "yarn build:production && yarn upload",
    "build": "rm -rf ./dist && yarn build:backend && yarn build:frontend",
    "build:backend": "tsc --outDir ./dist",
    "build:frontend": "nuxt build && mv ./nuxt ./dist/nuxt && cp ./package.json ./.env* ./dist",
    "upload": "rsync -r ./dist/* root@128.199.216.159:/root/hugsnan-database",
    "generate": "nuxt generate",
    "test": "JEST=1 jest --forceExit",
    "rm:cache": "rm -rf node_modules/.cache ./dist ./nuxt",
    "debug": "SKIP_NUXT=1 cross-env NODE_ENV=development tsnd --inspect --respawn ./server/index.ts",
    "debug:watch": "nodemon --watch server --exec yarn debug"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/style-resources": "^0.1.2",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.11",
    "@types/consola": "^1.0.0",
    "@types/form-data": "^2.2.1",
    "@types/http-assert": "^1.4.0",
    "@types/iconv-lite": "^0.0.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.40",
    "@types/koa2-cors": "^2.0.1",
    "@types/lodash": "^4.14.123",
    "@types/mongodb": "^3.1.25",
    "@types/supertest": "^2.0.7",
    "@types/tesseract.js": "^0.0.2",
    "cheerio": "^1.0.0-rc.3",
    "dotenv-extended": "^2.4.0",
    "form-data": "^2.3.3",
    "fs": "^0.0.1-security",
    "http-assert": "^1.4.1",
    "iconv-lite": "^0.4.24",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^3.2.1",
    "koa-cookie": "^1.0.0",
    "koa-ignore": "^1.0.1",
    "koa-logger": "^3.2.0",
    "koa-mongo": "^1.8.0",
    "koa-mount": "^4.0.0",
    "koa-respond": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-fetch": "^2.3.0",
    "pdf-parse": "^1.1.1",
    "simple-crypto-js": "^2.0.2",
    "tesseract.js": "^1.0.14",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.35"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3"
  }
}
